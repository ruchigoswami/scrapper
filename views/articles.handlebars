<ul> 
    {{#each data}}
      <li class="list-group-item">
        <h4>{{title}}</h4>
        <h6>{{blurb}}</h6>
        <a href="http://foxnews.com{{link}}">foxnews.com{{link}}</a>
        <button id="{{id}}" class="btn btn-info btn-save-article" style="float: right;">Save Article</button>
      </li>          
    {{/each}}
</ul>